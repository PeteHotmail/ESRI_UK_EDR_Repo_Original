{
  "name": "edr-proxy",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "serve": "node dist/index.js",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint && npm run copy-static-assets",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "prestart": "npm run build",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "jest --forceExit --coverage --verbose --reporters=default --reporters=jest-junit",
    "watch-test": "npm run test -- --watchAll",
    "start": "npm run serve",
    "debug": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "Apache 2.0",
  "dependencies": {
    "@esri/arcgis-to-geojson-utils": "^1.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "helmet": "^3.22.1",
    "swagger-ui-express": "^4.1.4",
    "terraformer": "^1.0.12",
    "terraformer-wkt-parser": "^1.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/concurrently": "^5.1.0",
    "@types/cors": "^2.8.6",
    "@types/eslint": "^6.8.0",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/shelljs": "^0.8.7",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "jest": "^25.4.0",
    "jest-junit": "^10.0.0",
    "nodemon": "^2.0.3",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  }
}
